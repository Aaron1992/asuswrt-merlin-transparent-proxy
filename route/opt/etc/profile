alias ls='ls --color=auto'
alias scripts='cd /jffs/scripts'
alias etc='cd /opt/etc'
alias init.d='cd /opt/etc/init.d'
alias ipl='iptables -L -n -v'
alias grep='/opt/bin/grep --color=auto'
alias port='netstat -antu |grep'
alias p='ps |grep'
alias e='nano'
alias rd='dnsmasq --test && kill $(cat /var/run/dnsmasq.pid) && dnsmasq --log-async'
alias wget='wget --no-check-certificate'
alias iptables='/opt/sbin/iptables'
alias ipset='/opt/sbin/ipset'
